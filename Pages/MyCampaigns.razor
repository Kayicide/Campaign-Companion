@page "/mycampaigns"
@using DnD_Combat_Turn_Tracker.Components

<MudContainer Class="mt-16 px-8" MaxWidth="MaxWidth.False">
    <MudGrid>
        @foreach (var campaign in CampaignList)
        {
            <CampaignComponent Campaign="campaign"></CampaignComponent>
        }
    </MudGrid>
</MudContainer>