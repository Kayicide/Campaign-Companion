<MudCard Class="cursor-pointer" Elevation="@ElevationLevel" @onmouseover="OnMouseOver" @onmouseout="OnMouseOut" @onclick="OnClickCampaign">
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">@Campaign.Name</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText>This day everything happened.</MudText>
    </MudCardContent>
</MudCard>
